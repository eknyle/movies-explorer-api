{
  "env": {
    "es2021": true
  },
  "extends": "airbnb-base"
  ,
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "linebreak-style": 0,
    "no-control-regex": 0,
    "no-useless-escape": 0
  }
}
